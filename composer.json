{
    "name": "smashed-egg/laravel-in-memory-auth",
    "description": "This package allows you to Authenticate an admin area without the need for a database.",
    "keywords": ["php", "laravel", "auth", "authenication"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/smashed-egg/laravel-in-memory-auth/issues",
        "source": "https://github.com/smashed-egg/laravel-in-memory-auth"
    },
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^9.21|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^9.21|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.21|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "SmashedEgg\\LaravelInMemoryAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmashedEgg\\LaravelInMemoryAuth\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tom Ellis"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "SmashedEgg\\LaravelInMemoryAuth\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
